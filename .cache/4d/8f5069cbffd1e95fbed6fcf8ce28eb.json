,"original":{"line":38,"column":15},"generated":{"line":1,"column":1478}},{"source":"scripts/homework-13/01-pie.js","name":"then","original":{"line":39,"column":3},"generated":{"line":1,"column":1507}},{"source":"scripts/homework-13/01-pie.js","name":"ready","original":{"line":39,"column":8},"generated":{"line":1,"column":1512}},{"source":"scripts/homework-13/01-pie.js","name":"catch","original":{"line":40,"column":3},"generated":{"line":1,"column":1515}},{"source":"scripts/homework-13/01-pie.js","original":{"line":40,"column":9},"generated":{"line":1,"column":1521}},{"source":"scripts/homework-13/01-pie.js","name":"err","original":{"line":40,"column":9},"generated":{"line":1,"column":1530}},{"source":"scripts/homework-13/01-pie.js","name":"console","original":{"line":40,"column":16},"generated":{"line":1,"column":1533}},{"source":"scripts/homework-13/01-pie.js","name":"console","original":{"line":40,"column":16},"generated":{"line":1,"column":1540}},{"source":"scripts/homework-13/01-pie.js","name":"log","original":{"line":40,"column":24},"generated":{"line":1,"column":1548}},{"source":"scripts/homework-13/01-pie.js","original":{"line":40,"column":28},"generated":{"line":1,"column":1552}},{"source":"scripts/homework-13/01-pie.js","name":"err","original":{"line":40,"column":43},"generated":{"line":1,"column":1566}}],"sources":{"scripts/homework-13/01-pie.js":"import * as d3 from 'd3'\n\nconst margin = { top: 30, left: 30, right: 30, bottom: 30 }\nconst height = 400 - margin.top - margin.bottom\nconst width = 780 - margin.left - margin.right\n\nconst svg = d3\n  .select('#chart-1')\n  .append('svg')\n  .attr('height', height + margin.top + margin.bottom)\n  .attr('width', width + margin.left + margin.right)\n  .append('g')\n  .attr('transform', 'translate(' + margin.left + ',' + margin.top + ')')\n  .attr('transform', `translate(${width / 2},${height / 2})`)\n\n// At the very least you'll need scales, and\n// you'll need to read in the file. And you'll need\n// and svg, too, probably.\n\nconst pie = d3.pie().value(function(d) {\n  return d.minutes\n})\n\nconst radius = 120\n\nconst arc = d3\n  .arc()\n  .innerRadius(0)\n  .outerRadius(radius)\n\nconst labelArc = d3\n  .arc()\n  .innerRadius(0)\n  .outerRadius(radius + 130)\n\nconst colorScale = d3.scaleOrdinal().range(['#98DC4B', 'purple', '#fec44f'])\n\nd3.csv(require('/data/time-breakdown.csv'))\n  .then(ready)\n  .catch(err => console.log('Failed with', err))\n\nfunction ready(datapoints) {\n  console.log(pie(datapoints))\n\n  // const names = pie(datapoints).map(d => d.data.task)\n  // labelArc.domain(names)\n\n  svg\n    .selectAll('path')\n    .data(pie(datapoints))\n    .enter()\n    .append('path')\n    .attr('d', d => arc(d))\n    .attr('fill', d => colorScale(d.data.task))\n\n  svg\n    .selectAll('.label')\n    .data(pie(datapoints))\n    .enter()\n    .append('text')\n    .text(d => d.data.task)\n    .attr('d', d => labelArc(d))\n    .attr('transform', function(d) {\n      return `translate(${labelArc.centroid(d)})`\n    })\n    .attr('text-anchor', function(d) {\n      if (d.startAngle > Math.PI) {\n        return 'end'\n      } else {\n        return 'start'\n      }\n    })\n}\n"},"lineCount":null}},"error":null,"hash":"53a6079926e2d0f11c7b953ccd223af4","cacheData":{"env":{}}}