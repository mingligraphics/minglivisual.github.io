:7},"generated":{"line":1,"column":1705}},{"source":"scripts/homework-13/02-pie-multiples.js","original":{"line":34,"column":15},"generated":{"line":1,"column":1713}},{"source":"scripts/homework-13/02-pie-multiples.js","name":"then","original":{"line":35,"column":3},"generated":{"line":1,"column":1746}},{"source":"scripts/homework-13/02-pie-multiples.js","name":"ready","original":{"line":35,"column":8},"generated":{"line":1,"column":1751}},{"source":"scripts/homework-13/02-pie-multiples.js","name":"catch","original":{"line":36,"column":3},"generated":{"line":1,"column":1754}},{"source":"scripts/homework-13/02-pie-multiples.js","original":{"line":36,"column":9},"generated":{"line":1,"column":1760}},{"source":"scripts/homework-13/02-pie-multiples.js","name":"err","original":{"line":36,"column":9},"generated":{"line":1,"column":1769}},{"source":"scripts/homework-13/02-pie-multiples.js","name":"console","original":{"line":37,"column":4},"generated":{"line":1,"column":1772}},{"source":"scripts/homework-13/02-pie-multiples.js","name":"log","original":{"line":37,"column":12},"generated":{"line":1,"column":1780}},{"source":"scripts/homework-13/02-pie-multiples.js","original":{"line":37,"column":16},"generated":{"line":1,"column":1784}},{"source":"scripts/homework-13/02-pie-multiples.js","name":"err","original":{"line":37,"column":31},"generated":{"line":1,"column":1798}}],"sources":{"scripts/homework-13/02-pie-multiples.js":"import * as d3 from 'd3'\n\nconst margin = { top: 30, left: 30, right: 30, bottom: 30 }\nconst height = 400 - margin.top - margin.bottom\nconst width = 780 - margin.left - margin.right\n\nconst svg = d3\n  .select('#chart-2')\n  .append('svg')\n  .attr('height', height + margin.top + margin.bottom)\n  .attr('width', width + margin.left + margin.right)\n  .append('g')\n  .attr('transform', 'translate(' + margin.left + ',' + margin.top + ')')\n\nconst pie = d3.pie().value(function(d) {\n  return d.minutes\n})\n\nconst radius = 80\n\nconst arc = d3\n  .arc()\n  .innerRadius(0)\n  .outerRadius(radius)\n\nconst colorScale = d3.scaleOrdinal().range(['#98DC4B', '#c994c7', '#fec44f'])\n\nconst pieScale = d3\n  .scalePoint()\n  .range([0, width])\n  .padding(0.5)\n\n// Read in files\nd3.csv(require('/data/time-breakdown-all.csv'))\n  .then(ready)\n  .catch(err => {\n    console.log('Failed with', err)\n  })\n\nfunction ready(datapoints) {\n  const names = datapoints.map(d => d.project)\n  pieScale.domain(names)\n\n  const colorNames = datapoints.map(d => d.task)\n  colorScale.domain(colorNames)\n\n  const nested = d3\n    .nest()\n    .key(function(d) {\n      return d.project\n    })\n    .entries(datapoints)\n\n  console.log('The data is', nested)\n\n  svg\n    .selectAll('g')\n    .data(nested)\n    .enter()\n    .append('g')\n    .attr('transform', function(d) {\n      return `translate(${pieScale(d.key)},100)`\n    })\n    .each(function(d) {\n      const name = d.key\n      const datapoints = d.values\n      console.log(d)\n      // What svg are we in? Let's grab it\n      const g = d3.select(this)\n\n      g.selectAll('path')\n        .data(pie(datapoints))\n        .enter()\n        .append('path')\n        .attr('d', d => arc(d))\n        .attr('fill', d => colorScale(d.data.task))\n\n      console.log('The new data is', pie(datapoints))\n\n      g.selectAll('.title')\n        .data(pie(datapoints))\n        .enter()\n        .append('text')\n        .text(name)\n        .attr('transform', function(d) {\n          return `translate(${pieScale(d.key)},0)`\n        })\n        .attr('dy', 120)\n        .attr('dx', -20)\n    })\n}\n"},"lineCount":null}},"error":null,"hash":"86d9e3a2ed81df9e32147731caaec019","cacheData":{"env":{}}}